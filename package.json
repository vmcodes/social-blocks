    "eslint-plugin-prettier": "^5.0.0",
    "jest": "29.6.1",
    "pm2": "^5.3.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
