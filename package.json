    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "ethers": "^6.7.1",
    "jsonwebtoken": "^9.0.1",
    "jwt-payloader": "^1.0.1",
